{
  "options": {
    "disable_signup": false,
    "name": "email",
    "email": {
      "syntax": "liquid",
      "from": "{{ application.name }} <root@auth0.com>",
      "subject": "Welcome to passwordless",
      "body": "my html"
    },
    "totp": {
      "time_step": 180,
      "length": 6
    },
    "brute_force_protection": true
  },
  "strategy": "email",
  "name": "email",
  "is_domain_connection": false,
  "enabled_clients": [
    "auth0-deploy-cli-extension"
  ]
}
